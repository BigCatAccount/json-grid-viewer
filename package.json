{
  "name": "json-viewer",
  "publisher": "DutchIgor",
  "displayName": "JSON grid viewer",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.46.0"
  },
  "main": "./editor/main.js",
  "activationEvents": [
    "onCustomEditor:jsonGridViewer.json"
  ],
  "contributes": {
    "customEditors": [
      {
        "viewType": "jsonGridViewer.json",
        "displayName": "JSON Grid",
        "selector": [
          {
            "filenamePattern": "*.json"
          }
        ],
        "priority": "option"
      }
    ]
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.18.0",
    "vue": "^3.2.13"
  },
  "devDependencies": {
    "@types/vscode": "^1.46.0",
    "@vue/cli-plugin-babel": "~4.5.13",
    "@vue/cli-plugin-eslint": "~4.5.13",
    "@vue/cli-service": "~4.5.13",
    "@vue/compiler-sfc": "^3.2.13",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^7.0.0-0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "files": [
    "editor/*",
    "webview/*",
    "demo.webp"
  ]
}